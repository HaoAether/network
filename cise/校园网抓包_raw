			$("#slefChangeGroup").click(function(){
				
				//........................................................................
				if ($("#userId").val() == "") {
					$.tooltip('.....................', tripFailTime, false);
					$("#userId").focus();
					return false;
				}

				if ($("#passwd").val() == "") {
					$.tooltip('...............', tripFailTime, false);
					$("#passwd").focus();
					return false;
				}

				var userId = $("#userId").val();
				var passwd = $("#passwd").val();
				var pageid = $("#pageid").val();
				$.ajax({
					url : "/httpservice/checkUserPwdGeneral.do",
					type : "POST",
					data : {
						userId : userId,
						passwd : passwd,
						pageid : pageid
					},
					dataType : "json",
					success : function(json) {
						switch (json.check) {
							case '0':
								$.tooltip('............,.................................', tripFailTime, false);
								setTimeout(toChangeGroup, 2000);
								break;
							case '1':
								$.tooltip('............', tripFailTime, false);
								break;
							case '2':
								$.tooltip('...............', tripFailTime, false);
								break;
							case '3':
								$.tooltip('............', tripFailTime, false);
								break;
						}
					},
					error : function(json) {
						$.tooltip('..............................!', tripFailTime, false);
					}
				});
				
			});


