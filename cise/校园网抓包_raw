		var openWXInterval;
		function sleepAndOpenWX(second) {
			countSecond = second;
			openWXInterval = setInterval(function() {
				if (countSecond == 0) {
					if(window.console){
							console.log("..................");
					}
					openWxClient();
					clearTimeout(openWXInterval);
				}else {
					if(window.console)
						console.log("......" + countSecond + "..................");
				}
				countSecond = countSecond - 1;
			}, 1000);
		}

