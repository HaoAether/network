		function sleepAndGoCharge(second,accountId) {
			var wlanuserip=$("#wlanuserip").val();
			var wlanacname=$("#wlanacname").val();
			console.log("........................wlanuserip:"+wlanuserip+",........."+accountId);
			// ...........................
			if(isIntranetIp()){
				authPortalWx(); // ............
			}
			countSecond = second;
			openWXInterval = setInterval(function() {
				if (countSecond == 0) {
					if(window.console)
						console.log("........................");
					clearTimeout(openWXInterval);
					var chageUrl="/self/onlineCharge.do?accountId="+accountId+"&wlanuserip="+wlanuserip+"&wlanacname="+wlanacname+"&distoken="+distoken;
					var mobile_flag = isMobile();
					var width='90%',height='90%';
			        if(mobile_flag){
						var bankReturHostIp= 'http://';
						// .....................
						if(bankReturHostIp!="http://") {
							chageUrl=bankReturHostIp+chageUrl; // ............+............
						}
						window.location.href = chageUrl;
			        }else{
						console.log("PC.........URL:"+chageUrl);
						 layer.open({
							type:2,
							title: 'PORTAL............',
							maxmin: false,//...............
							btn:[],
							closeBtn: 1,
							shade: 0.6,
							shadeClose: false, //chick shadeClose.....................
							area: [width,height],
							content: chageUrl+"?accountId="+accountId
						});
					}
				}else {
					if(window.console)
						console.log("......" + countSecond + "........................");
				}
				countSecond = countSecond - 1;
			}, 1000);
		}


		$("#recharge").on("click",function(){
