		var tservertypeid = $("#tservertypeid").val();
		var portalVer = $("#portalVer").val();
		var autouserid = 'radius_share';
		var autopasswd = 'radius_share';

		if ($('#autouserid').length > 0 && $('#autouserid').val() != '') {
			autouserid = $("#autouserid").val();
